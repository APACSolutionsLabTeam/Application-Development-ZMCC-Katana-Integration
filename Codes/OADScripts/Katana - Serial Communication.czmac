serialPort.Close()    

## User-defined function to output the 2nd string read from the serial buffer
def out_string(input_str):
    
    output_str =""
    
    for i in range(2):
        serialPort.WriteLine(input_str)
        time.sleep(0.1)
        output_str = serialPort.ReadExisting()
    #print(output_str)
    
    return output_str        

