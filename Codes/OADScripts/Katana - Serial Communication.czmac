##
import clr
clr.AddReference('System')
from System import *
import re
import time
##
serialPort = IO.Ports.SerialPort("COM6")
serialPort.BaudRate = 115200
serialPort.DataBits = 8
serialPort.Close()
serialPort.Open()
print("Port Open")

for i in range(1): 
    
    s1 = out_string("K?\n")
    print("1:K? " ,str(i), s1)
    
    s2 = out_string("KE\n")
    print("2:KE " ,str(i), s2)
        
    s3 = out_string("KS\n")
