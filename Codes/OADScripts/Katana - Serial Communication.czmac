    s4 = out_string("KST\n")
    print("4:KST " ,str(i), s4)
    
    s5 = out_string("KT250000,100\n")
    print("5:KT250000,100 " ,str(i), s5)
    
    s6 = out_string("KO40590\n")
    print("6:KO40590 " ,str(i), s6)
    
